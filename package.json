{
  "name": "webpack-learing",
  "version": "1.0.0",
  "description": "Learning Webpack",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "built": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "built:local": "webpack --mode production",
    "built:external": "webpack --config ./external/webpack.config.js --mode development",
    "built:multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "built:css": "webpack --config ./css-style-loader/webpack.config.js",
    "built:plugins": "webpack --config ./plugins/webpack.config.js"
  },
